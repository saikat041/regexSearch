{
    "manifest_version": 2,
    "name": "Regex Search Extension",
    "version": "1.0",
    "icons": {
        "128": "res/icon.png"
    },
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "content_script.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    }
}